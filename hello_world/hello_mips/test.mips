    .text
    .global main
main:
    la      $t1, str
    add     $t2, $t1, 5
    li      $t3, 1
loop:
    bge     $t1, $t2, end
    lb      $t4, ($t1)
    lb      $t5, ($t2)
    bne     $t4, $t5, notmatch
    add     $t1, 1
    sub     $t2, 1
notmatch:
    li      $t3, 0
endloop:
    jal     exit
    .data
str:.ascii  "abcba"
